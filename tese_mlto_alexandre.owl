<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://www.ime.usp.br/~alexandre/mlto"
         xmlns="http://www.ime.usp.br/~alexandre/mlto#">

<owl:Ontology rdf:about="http://www.ime.usp.br/~alexandre/mlto"/>

<owl:ObjectProperty rdf:about="#has_state">
  <rdfs:domain rdf:resource="#Execution"/>
  <rdfs:range rdf:resource="#State"/>
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_symptom">
  <rdfs:domain rdf:resource="#Execution"/>
  <rdfs:range rdf:resource="#Symptom"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#accuracy">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Execution"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#batch_size">
  <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  <rdfs:domain rdf:resource="#Execution"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#Execution">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#State">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Symptom">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#Infected">
  <rdfs:subClassOf rdf:resource="#State"/>
</owl:Class>

<owl:Class rdf:about="#Healthy">
  <rdfs:subClassOf rdf:resource="#State"/>
</owl:Class>

<owl:Class rdf:about="#GradientInstability">
  <rdfs:subClassOf rdf:resource="#Symptom"/>
</owl:Class>

<owl:NamedIndividual rdf:about="#EXP_001_STOCHASTIC">
  <rdf:type rdf:resource="#Execution"/>
  <has_state rdf:resource="#Infected_State_EXP_001_STOCHASTIC"/>
  <has_symptom rdf:resource="#Gradient_Noise_EXP_001_STOCHASTIC"/>
  <batch_size rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</batch_size>
  <accuracy rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.48</accuracy>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Infected_State_EXP_001_STOCHASTIC">
  <rdf:type rdf:resource="#Infected"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Gradient_Noise_EXP_001_STOCHASTIC">
  <rdf:type rdf:resource="#GradientInstability"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#EXP_002_MINIBATCH">
  <rdf:type rdf:resource="#Execution"/>
  <has_state rdf:resource="#Healthy_State_EXP_002_MINIBATCH"/>
  <batch_size rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">50</batch_size>
  <accuracy rdf:datatype="http://www.w3.org/2001/XMLSchema#decimal">0.97</accuracy>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Healthy_State_EXP_002_MINIBATCH">
  <rdf:type rdf:resource="#Healthy"/>
</owl:NamedIndividual>


</rdf:RDF>
